<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="triangle" class="org.vigilantex.demo.Triangle" autowire="byName"/>

    <bean id="pointA" class="org.vigilantex.demo.Point">

        <property name="x" value="1"/>
        <property name="y" value="2"/>
        
    </bean>
    <bean id="pointB" class="org.vigilantex.demo.Point">

        <property name="x" value="3"/>
        <property name="y" value="4"/>

    </bean>
    <bean id="pointC" class="org.vigilantex.demo.Point">

        <property name="x" value="5"/>
        <property name="y" value="6"/>

    </bean>
    <alias name="triangle" alias="t-alias"/>


</beans>